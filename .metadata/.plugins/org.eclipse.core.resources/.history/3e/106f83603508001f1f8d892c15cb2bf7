package com.kh.goodsShop;

import java.util.*;

public class 컴퓨터가게 {
	public static void main(String[] args) {
		
		//모든제품 HashMap
		Map<String,Integer> 모든제품 = new HashMap<>();
		//상품추가 put
		//노트북 700000
		//데스크탑 20000
		//모니터 5000
		//마우스 3000
		//키보드 2000
		상품추가.put("노트북",70000);
		상품추가.put("데스크탑",70000);
		상품추가.put("모니터",70000);
		상품추가.put("마우스",70000);
		상품추가.put("키보드",70000);
		
		
		System.out.println("컴퓨터 상품 목록");
		//Entry 사용해서 출력
		
	}
}
